{
  "name": "strava-api-lib",
  "version": "1.0.0",
  "private": true,
  "description": "A TypeScript library to interact with the Strava API.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "prepublishOnly": "npm run build",
    "test:lib": "jest",
    "test:app": "npm run test --prefix ./my-strava-app",
    "test": "npm run test:lib && npm run test:app"
  },
  "workspaces": [
    "my-strava-app"
  ],
  "keywords": [
    "Strava",
    "API",
    "Library",
    "TypeScript",
    "React"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^28.0.0",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^28.0.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "es-abstract": "^1.21.0",
    "string.prototype.matchall": "^4.0.8"
  }
}